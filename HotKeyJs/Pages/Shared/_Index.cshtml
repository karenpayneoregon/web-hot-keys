@{
}
<form class="mt-lg-4 mb-3">

    <div class="row">
        <div class="col fw-bold">
            Keys overridden defaults
        </div>
    </div>

    <div class="row w-75">
        <ul class="list-group">
            <li class="list-group-item"><kbd>alt</kbd> + <kbd>h</kbd>&nbsp; open home page</li>
            <li class="list-group-item"><kbd>alt</kbd> + <kbd>a</kbd>&nbsp; open about page</li>
            <li class="list-group-item"><kbd>alt</kbd> + <kbd>0</kbd>&nbsp; skip link</li>
            <li class="list-group-item"><kbd>alt</kbd> + <kbd>1</kbd>&nbsp; add css debug</li>
            <li class="list-group-item"><kbd>alt</kbd> + <kbd>2</kbd>&nbsp; remove css debug</li>
            <li class="list-group-item"><kbd>alt</kbd> + <kbd>3</kbd>&nbsp; show/hide help</li>
            <li class="list-group-item"><kbd>ctrl</kbd> + <kbd>q</kbd> &nbsp; Submit form</li>
        </ul>
    </div>
</form>